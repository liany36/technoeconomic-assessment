  "paths": {
    "/genericPowerOnly": {
      "post": {
        "tags": [
          "Generic Biomass Power Plant"
        ],
        "summary": "Runs GPO from given input",
        "description": "This route is used for generic-power-only (GPO) model",
        "operationId": "runGPO",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "$ref": "#/definitions/GenericPowerOnlyInputMod"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/GenericPowerOnlyOutputMod"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    },
    "/genericCombinedHeatPower": {
      "post": {
        "tags": [
          "Generic Combined Heat And Power"
        ],
        "summary": "Runs CHP from given input",
        "description": "This route is used for generic-combine-heat-power (CHP) model",
        "operationId": "runCHP",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "$ref": "#/definitions/GenericCombinedHeatPowerInputMod"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/GenericCombinedHeatPowerOutputMod"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    },
    "/gasificationPower": {
      "post": {
        "tags": [
          "Biomass Gasification/Power Generation"
        ],
        "summary": "Runs GP from given input",
        "description": "This route is used for gasification-power (GP) model",
        "operationId": "runGP",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "$ref": "#/definitions/GasificationPowerInputMod"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/GasificationPowerOutputMod"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    }
  },
